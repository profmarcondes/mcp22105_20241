.include "macros.inc"
.include "stdlib.inc"
.include "pacman.inc"
.include "sprites.inc"
.include "ringbuffer.inc"
.include "kernel.inc"

.eqv STACK_INIT 0x7FFFEFFC

.data
.align 2
key_rb: .space 28

.text 0x00400000 ## System Init Code
init:
	la $sp, STACK_INIT
	
	jal main
	syscall_exit
  		
.text 0x00420000  ## System lib
#=============================================================
# void enable_int
enable_int:

	jr 		$ra
#=============================================================

#=============================================================
# void disable_int
disable_int:

	jr 		$ra
#=============================================================

#=============================================================
# void enable_irq(int irq_num)
enable_irq:

	jr 		$ra
#=============================================================

#=============================================================
# void disable_irq(int irq_num)
disable_irq:

	jr 		$ra
#=============================================================

#=============================================================
# void enable_keyboard_int
enable_keyboard_int:

	jr 		$ra	
#=============================================================

#=============================================================
# void set_pixel(X, Y, color)
set_pixel:

   jr  $ra
#=============================================================

#=============================================================
# char getchar();
getchar:

    jr $ra
#=============================================================

#=============================================================
# void putchar(char value);
putchar:

    jr $ra

  jr $ra
#=============================================================
